@model MyAcademyCarBook.EntityLayer.Concrete.EndService
@{
    ViewData["Title"] = "AddEndService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Yeni Hizmet Girişi</h3>

<form method="post">
    <label>Servis Başlığı</label>
    <input type="text" style="border:2px solid #2222" asp-for="ServiceTitle" class="form-control" />
    <br />
    <label>Servis Açıklaması</label>
    <input type="text" style="border:2px solid #2222" asp-for="ServiceDescription" class="form-control" />
    <br />
    <label>Servis Resimi</label>
    <input type="text" style="border:2px solid #2222" asp-for="ServiceImage" class="form-control" />
    <br />
  
    <button class="btn btn-primary">Hizmeti Kaydet</button>




</form>