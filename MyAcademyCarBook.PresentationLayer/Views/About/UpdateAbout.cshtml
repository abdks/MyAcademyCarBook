@model MyAcademyCarBook.EntityLayer.Concrete.Team

@{
    ViewData["Title"] = "UpdateAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Takım Arkadaşı Güncelleme Sayfası</h3>
<form method="post">
    <input type="hidden" asp-for="TeamID" />
    <label>Resim </label>
    <input type="text" asp-for="ProfilePhoto" class="form-control" style="border:2px solid #2222" />
    <br />
    <label>Unvan</label>
    <input type="text" asp-for="Unvan" class="form-control" style="border:2px solid #2222" />
    <br />
    <label>Ad</label>
    <input type="text" asp-for="Ad" class="form-control" style="border:2px solid #2222" />
    <br />
    <label>Soyad</label>
    <input type="text" asp-for="Soyad" class="form-control" style="border:2px solid #2222" />
    <br />
    <label>Açıklama</label>
    <input type="text" asp-for="Description" class="form-control" style="border:2px solid #2222" />
    <br />
   

    <button class="btn btn-success">Kişiyi Güncelle</button>
</form>
