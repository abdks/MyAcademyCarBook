@model MyAcademyCarBook.EntityLayer.Concrete.Team
@{
    ViewData["Title"] = "AddAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Yeni Takım Arkadaşı Girişi</h3>

<form method="post">
    <label>Resim</label>
    <input type="text" style="border:2px solid #2222" asp-for="ProfilePhoto" class="form-control" />
    <br />
    <label>Unvan</label>
    <input type="text" style="border:2px solid #2222" asp-for="Unvan" class="form-control" />
    <br />
    <label>Ad</label>
    <input type="text" style="border:2px solid #2222" asp-for="Ad" class="form-control" />
    <br />
    <label>Soyad</label>
    <input type="text" style="border:2px solid #2222" asp-for="Soyad" class="form-control" />
    <br />
    <label>Açıklama</label>
    <input type="text" style="border:2px solid #2222" asp-for="Description" class="form-control" />
    <br />

    <button class="btn btn-primary">Kişiyi Kaydet</button>




</form>