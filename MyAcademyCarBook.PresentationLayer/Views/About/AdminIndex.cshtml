@model List<MyAcademyCarBook.EntityLayer.Concrete.Team>

@{
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<br />
<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Resim</th>
        <th>Unvan</th>
        <th>Ad Soyad</th>
        <th>Açıklama</th>

        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.TeamID</td>
            <td><img src="@item.ProfilePhoto" width="60px"height="60px" /></td>
            <td>@item.Unvan</td>
            <td>@item.Ad @item.Soyad</td>
            <td>@item.Description</td>
            <td><a href="/About/DeleteAbout/@item.TeamID" class="btn btn-danger">Sil</a></td>
            <td><a href="/About/UpdateAbout/@item.TeamID" class="btn btn-success">Güncelle</a></td>
        </tr>
    }

</table>
<a href="/About/AddAbout" class="btn btn-primary">Yeni Takım Arkadaşı Girişi</a>

