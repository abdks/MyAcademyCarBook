@model MyAcademyCarBook.EntityLayer.Concrete.Brand

@{
    ViewData["Title"] = "UpdateBrand";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>İletişim Güncelleme Sayfası</h3>
<form method="post">
    <input type="hidden" asp-for="BrandID" />
    <label>İsim</label>
    <input type="text" asp-for="BrandName" class="form-control" style="border:2px solid #2222" />
    <br />


    <button class="btn btn-success">Güncelle</button>
</form>
