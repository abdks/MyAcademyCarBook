@model MyAcademyCarBook.EntityLayer.Concrete.Contact
@{
    ViewData["Title"] = "AddContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Yeni İletişim Girişi</h3>

<form method="post">
    <label>Adres</label>
    <input type="text" style="border:2px solid #2222" asp-for="Adress" class="form-control" />
    <br />
    <label>Telefon</label>
    <input type="text" style="border:2px solid #2222" asp-for="Tel" class="form-control" />
    <br />
    <label>Mail</label>
    <input type="text" style="border:2px solid #2222" asp-for="Mail" class="form-control" />
    <br />
    <label>Facebook</label>
    <input type="text" style="border:2px solid #2222" asp-for="Facebook" class="form-control" />
    <br />
    <label>İnstagram</label>
    <input type="text" style="border:2px solid #2222" asp-for="Instagram" class="form-control" />
    <br />
    <label>Twitter</label>
    <input type="text" style="border:2px solid #2222" asp-for="Twitter" class="form-control" />
    <br />
    <label>Linkedln</label>
    <input type="text" style="border:2px solid #2222" asp-for="Linkedln" class="form-control" />
    <br />

    <button class="btn btn-primary">Kaydet</button>




</form>