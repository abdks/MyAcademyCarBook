@model DashboardViewModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



        
  

                    <div class="row">

                        <!-- order-card start -->
                        <div class="col-md-6 col-xl-3">
                            <div class="card bg-c-blue order-card">
                                <div class="card-block">
                                    <h6 class="m-b-20">Araç Sayısı</h6>
                <h2 class="text-right"><i class="ti-car f-left"></i><span>@ViewBag.CarCount</span></h2>
                                    <p class="m-b-0">Son Güncelleme<span class="f-right">03/12/2023</span></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-xl-3">
                            <div class="card bg-c-green order-card">
                                <div class="card-block">
                                    <h6 class="m-b-20">Kategori Sayısı</h6>
                <h2 class="text-right"><i class="ti-tag f-left"></i><span>@ViewBag.categoryCount</span></h2>
                <p class="m-b-0">Son Güncelleme<span class="f-right">03/12/2023</span></p>
            </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-xl-3">
                            <div class="card bg-c-yellow order-card">
                                <div class="card-block">
                                    <h6 class="m-b-20">Marka Sayısı</h6>
                                    <h2 class="text-right"><i class="ti-reload f-left"></i><span>@ViewBag.brandCount</span></h2>
                <p class="m-b-0">Son Güncelleme<span class="f-right">03/12/2023</span></p>
            </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-xl-3">
                            <div class="card bg-c-pink order-card">
                                <div class="card-block">
                                    <h6 class="m-b-20">Gelen Mesaj</h6>
                <h2 class="text-right"><i class="ti-mail f-left"></i><span>@ViewBag.contactCount</span></h2>
                <p class="m-b-0">Son Güncelleme<span class="f-right">03/12/2023</span></p>
            </div>
                            </div>
                        </div>

                        <div class="col-sm-12">
                            <div class="card tabs-card">
                                <div class="card-block p-0">
                                    <!-- Nav tabs -->
                                    <ul class="nav nav-tabs md-tabs" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" data-toggle="tab" href="#home3" role="tab"><i class="fa fa-car"></i>Araçlar</a>
                                            <div class="slide"></div>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" data-toggle="tab" href="#profile3" role="tab"><i class="fa fa-key"></i>Kategoriler</a>
                                            <div class="slide"></div>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" data-toggle="tab" href="#messages3" role="tab"><i class="fa fa-play-circle"></i>Markalar</a>
                                            <div class="slide"></div>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" data-toggle="tab" href="#settings3" role="tab"><i class="fa fa-database"></i>Mesajlar</a>
                                            <div class="slide"></div>
                                        </li>
                                    </ul>
                                    <!-- Tab panes -->
                                    <div class="tab-content card-block">
                                        <div class="tab-pane active" id="home3" role="tabpanel">

                                            <div class="table-responsive">
                                                <table class="table">
                                                    <tr>
                                                       
                                                        <th>Km</th>
                                                        <th>Kapasite</th>
                                                        <th>Model</th>
                                                        <th>Yıl</th>
                                                        <th>Vites</th>
                                                       
                                                    </tr>
                                @foreach (var car in Model.Cars.Take(5))
                                {
                                    <tr>
                                        <td>@item.Km</td>
                                        <td>@item.PersonCount</td>
                                        <td>@item.Model</td>
                                        <td>@item.Year</td>
                                        <td>@item.GearType</td>
                                    </tr>
                                }

                                                </table>
                                            </div>
                                           
                                        </div>
